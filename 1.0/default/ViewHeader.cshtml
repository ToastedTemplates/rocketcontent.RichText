@inherits RocketContent.Components.RocketContentTokens<Simplisity.SimplisityRazor>
@using DNNrocketAPI.Components;
@using RocketPortal.Components;
@using Simplisity;

@{
    var portalData = new PortalLimpet(PortalUtils.GetCurrentPortalId());
    var appTheme = (AppThemeLimpet)Model.GetDataObject("apptheme");
    var sessionParams = (SessionParams)Model.SessionParamsData;

    var hbsInfo = new SimplisityInfo();
    hbsInfo.SetXmlProperty("genxml/engineurlwithprotocol", portalData.EngineUrlWithProtocol);
}

@RenderHandleBars(hbsInfo, appTheme, "viewheader.hbs", sessionParams.ModuleRef)

